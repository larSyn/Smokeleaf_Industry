<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  
    <!--========== Hemp Fiber ==========-->
	
	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempFiber</defName>
		<label>hemp fibers</label>
		<description>Leftover stalks and leaves from trimming the smokeleaf plant.</description>
		<graphicData>
			<texPath>Things/Items/MSI_hemp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<MarketValue>1.5</MarketValue>
			<Mass>0.25</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>12</DeteriorationRate>
      		<SharpDamageMultiplier>0.2</SharpDamageMultiplier>
      		<BluntDamageMultiplier>0.75</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<stuffAdjective>hemp</stuffAdjective>
			<categories>
				<li>Woody</li>
			</categories>
			<commonality>1.00</commonality>
			<color>(138,139,95)</color>
			<constructEffect>ConstructWood</constructEffect>
			<appearance>Planks</appearance>
			<soundImpactStuff>BulletImpactWood</soundImpactStuff>
			<soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>
			<soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>
			<statFactors>
				<MaxHitPoints>0.4</MaxHitPoints>
				<Beauty>1</Beauty>
				<Flammability>1</Flammability>
				<WorkToMake>0.75</WorkToMake>
				<WorkToBuild>0.75</WorkToBuild>
				<DoorOpenSpeed>1.25</DoorOpenSpeed>
				<MeleeWeapon_CooldownMultiplier>0.95</MeleeWeapon_CooldownMultiplier>
			</statFactors>
		</stuffProps>
		<tools>
			<li>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>6</power>
				<cooldownTime>1.25</cooldownTime>
			</li>
		</tools>
		<stackLimit>400</stackLimit>
		<thingCategories>
			<li>ResourcesRaw</li>
		</thingCategories>
		<researchPrerequisites>
			<li>SmokeleafCultivation</li>
		</researchPrerequisites>
	</ThingDef>
  
	<!--========== Smokeleaf Buds ==========-->
  
    <ThingDef ParentName="MSI_OrganicProductBase">
		<defName>SmokeleafBuds</defName>
		<label>smokeleaf buds</label>
		<description>The flower of the smokeleaf plant. Contains all the good stuff.</description>
		<graphicData>
			<texPath>Things/Items/MSI_bud</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MaxHitPoints>90</MaxHitPoints>
			<MarketValue>2</MarketValue>
			<Mass>0.05</Mass>
			<Flammability>1.5</Flammability>
			<DeteriorationRate>8</DeteriorationRate>
		</statBases>
		<stackLimit>400</stackLimit>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<researchPrerequisites>
			<li>SmokeleafCultivation</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== Smokeleaf Seeds ==========-->
  
	<ThingDef ParentName="MSI_OrganicProductBase">
		<defName>SmokeleafSeed</defName>
		<label>smokeleaf seed</label>
		<description>The seed of the smokeleaf plant.</description>
		<graphicData>
			<texPath>Things/Items/MSI_seed</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>1.5</MarketValue>
			<Flammability>0.5</Flammability>
		</statBases>
		<thingCategories>
			<li>PlantMatter</li>
		</thingCategories>
		<stackLimit>400</stackLimit>
		<researchPrerequisites>
			<li>SmokeleafCultivation</li>
		</researchPrerequisites>
	</ThingDef>

  <!--========== Hemp ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempFabric</defName>
		<label>hemp fabric</label>
		<description>Fabric woven from hemp fibers.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Cloth</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(138,139,95)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>110</MaxHitPoints>
			<MarketValue>6</MarketValue>
			<Mass>0.05</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>8</DeteriorationRate>
		</statBases>
		<stuffProps>
			<categories>
				<li>Fabric</li>
			</categories>
			<color>(138,139,108)</color>
			<commonality>1.4</commonality>
			<statOffsets>
				<ArmorRating_Blunt>0.05</ArmorRating_Blunt>
				<ArmorRating_Sharp>0.075</ArmorRating_Sharp>
				<ArmorRating_Heat>0.05</ArmorRating_Heat>
				<ArmorRating_Electric>0.025</ArmorRating_Electric>
			</statOffsets>
			<statFactors>
				<WorkToMake>1.3</WorkToMake>
				<ArmorRating_Blunt>1.25</ArmorRating_Blunt>
				<ArmorRating_Sharp>1.75</ArmorRating_Sharp>
				<ArmorRating_Heat>1.5</ArmorRating_Heat>
				<ArmorRating_Electric>1.0</ArmorRating_Electric>
				<Insulation_Cold>1.0</Insulation_Cold>
				<Insulation_Heat>1.5</Insulation_Heat>
			</statFactors>	
		</stuffProps>
		<thingCategories>
			<li>Textiles</li>
		</thingCategories>
		<researchPrerequisites>
			<li>SmokeleafProcessing</li>
			<li>ComplexClothing</li>
		</researchPrerequisites>
	</ThingDef>

  <!--========== Devilsleaf Fabric ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>DevilsleafFabric</defName>
		<label>devilsleaf fabric</label>
		<description>Hemp fabric blended with devilstrand to make a new textile with the best qualities of each material.</description>
		<graphicData>
			<texPath>Things/Item/Resource/Cloth</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(97,33,26)</color>
		</graphicData>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<MarketValue>12</MarketValue>
			<Mass>0.05</Mass>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>8</DeteriorationRate>
		</statBases>
		<stuffProps>
			<categories>
				<li>Fabric</li>
			</categories>
			<color>(97,33,26)</color>
			<commonality>1.4</commonality>
			<statOffsets>
				<ArmorRating_Blunt>0.075</ArmorRating_Blunt>
				<ArmorRating_Sharp>0.1</ArmorRating_Sharp>
				<ArmorRating_Heat>0.05</ArmorRating_Heat>
				<ArmorRating_Electric>0.05</ArmorRating_Electric>
			</statOffsets>
			<statFactors>
				<WorkToMake>1.3</WorkToMake>
				<ArmorRating_Blunt>1.5</ArmorRating_Blunt>
				<ArmorRating_Sharp>3.0</ArmorRating_Sharp>
				<ArmorRating_Heat>1.5</ArmorRating_Heat>
				<ArmorRating_Electric>2.5</ArmorRating_Electric>
				<Insulation_Cold>1.25</Insulation_Cold>
				<Insulation_Heat>1.5</Insulation_Heat>
			</statFactors>	
		</stuffProps>
		<thingCategories>
			<li>Textiles</li>
		</thingCategories>
		<researchPrerequisites>
			<li>IndustrialSmokeleafProcessing</li>
			<li>ComplexClothing</li>
		</researchPrerequisites>
	</ThingDef>

  <!--========== Hemp Biocomposite ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempBiocomposite</defName>
		<label>hemp biocomposite</label>
		<description>A biocomposite material made from hemp fibers and hemp fabric. Useful as a cheap, sustainable alternative for metals.</description>
		<graphicData>
			<texPath>Things/Items/MSI_bioComp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(130,130,100)</color>
		</graphicData>
    	<soundInteract>Metal_Drop</soundInteract>
    	<soundDrop>Metal_Drop</soundDrop>
    	<useHitPoints>false</useHitPoints>
		<statBases>
			<MarketValue>8</MarketValue>
			<Mass>0.25</Mass>
      		<SharpDamageMultiplier>0.55</SharpDamageMultiplier>
      		<BluntDamageMultiplier>0.85</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<categories>
				<li>Metallic</li>
			</categories>
			<color>(130,130,100)</color>
			<commonality>1.4</commonality>
      		<soundImpactStuff>BulletImpactMetal</soundImpactStuff>
      		<soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      		<soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
			<statFactors>
        		<MaxHitPoints>0.9</MaxHitPoints>
				<WorkToMake>1.3</WorkToMake>
       			<Flammability>0.25</Flammability>
        		<MeleeWeapon_CooldownMultiplier>0.75</MeleeWeapon_CooldownMultiplier>
			    <ArmorRating_Blunt>0.75</ArmorRating_Blunt>
			    <ArmorRating_Sharp>0.85</ArmorRating_Sharp>
			</statFactors>	
		</stuffProps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>AdvancedSmokeleafProcessing</li>
		</researchPrerequisites>
	</ThingDef>
  
    <!--========== Hempoline ==========-->
	
	<ThingDef ParentName="MSI_OrganicProductBase">
		<defName>Hempoline</defName>
		<label>hempoline</label>
		<description>A safe, stable fuel produced from refined smokeleaf stalks or seeds. Non-volatile, but goes bad eventually.</description>
		<graphicData>
			<texPath>Things/Items/MSI_hempoline</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>400</stackLimit>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<MarketValue>5</MarketValue>
			<Mass>0.05</Mass>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>120</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>IndustrialSmokeleafProcessing</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== CBD ==========-->
	  
	<ThingDef ParentName="MSI_ResourceBase">
		<defName>CBD</defName>
		<label>CBD</label>
		<description>A compound identified in the smokeleaf plant that has vast medical potential. CBD can be used on it's own or combined with other medicine to create an even more effective form of treatment.</description>
		<thingClass>Medicine</thingClass>
		<graphicData>
			<texPath>Things/Items/MSI_CBD</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<statBases>
			<MaxHitPoints>30</MaxHitPoints>
			<MarketValue>30</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>1.0</Flammability>
			<MedicalPotency>0.75</MedicalPotency>
			<DeteriorationRate>6</DeteriorationRate>
			<WorkToMake>750</WorkToMake>
		</statBases>
		<thingCategories>
			<li>SmokeleafIndCatDrugs</li>
		</thingCategories>
		<costList>
			<SmokeleafBuds>15</SmokeleafBuds>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugProductionSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>MedicinalsLab</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>3</Crafting>
				<Medicine>5</Medicine>
			</skillRequirements>
		</recipeMaker>
		<researchPrerequisites>
			<li>SmokeleafPharmacology</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== Infused Medicine ==========-->  

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>InfusedMedicine</defName>
		<label>infused medicine</label>
		<description>Medicine infused with CBD. More effective than regular medicine.</description>
		<thingClass>Medicine</thingClass>
		<graphicData>
			<texPath>Things/Items/MSI_medicine</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<MarketValue>36</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>1.0</Flammability>
			<MedicalPotency>1.50</MedicalPotency>
			<DeteriorationRate>6</DeteriorationRate>
			<WorkToMake>750</WorkToMake>
		</statBases>
		<thingCategories>
			<li>Medicine</li>
		</thingCategories>
		<costList>
			<Medicine>1</Medicine>
			<CBD>2</CBD>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugProductionSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>MedicinalsLab</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>6</Crafting>
				<Medicine>12</Medicine>
			</skillRequirements>
		</recipeMaker>
		<researchPrerequisites>
			<li>SmokeleafPharmacology</li>
		</researchPrerequisites>
	</ThingDef>  
	  
</Defs>