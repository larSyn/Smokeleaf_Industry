  <!--========== Hemp Biocomposite ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempBiocomposite</defName>
		<label>hemp biocomposite</label>
		<description>A biocomposite material made from hemp fibers, hemp fabric and biocoal. Useful as a sustainable alternative for metals.</description>
		<graphicData>
			<texPath>Things/Items/MSI_bioComp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(123,123,96)</color>
		</graphicData>
    	<soundInteract>Metal_Drop</soundInteract>
    	<soundDrop>Metal_Drop</soundDrop>
    	<useHitPoints>false</useHitPoints>
		<statBases>
			<MarketValue>10</MarketValue>
			<Mass>0.25</Mass>
	      	<StuffPower_Armor_Sharp>0.9</StuffPower_Armor_Sharp>
	      	<StuffPower_Armor_Blunt>0.45</StuffPower_Armor_Blunt>
	      	<StuffPower_Armor_Heat>0.72</StuffPower_Armor_Heat>
	      	<StuffPower_Insulation_Cold>3</StuffPower_Insulation_Cold>
	      	<StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat>
	      	<SharpDamageMultiplier>1</SharpDamageMultiplier>
	      	<BluntDamageMultiplier>1</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<categories>
				<li>Metallic</li>
			</categories>
			<color>(123,123,96)</color>
			<commonality>1.4</commonality>
      		<soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      		<soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      		<soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
			<statFactors>
        		<MaxHitPoints>0.9</MaxHitPoints>
				<WorkToMake>1.3</WorkToMake>
       			<Flammability>0.25</Flammability>
        		<MeleeWeapon_CooldownMultiplier>0.75</MeleeWeapon_CooldownMultiplier>
			    <ArmorRating_Blunt>0.75</ArmorRating_Blunt>
			    <ArmorRating_Sharp>0.85</ArmorRating_Sharp>
			</statFactors>	
		</stuffProps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>AdvancedSmokeleafProcessing</li>
		</researchPrerequisites>
	</ThingDef>

  <!--========== Carbonized Hemp ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempBiocoal</defName>
		<label>hemp biocoal</label>
		<description>The result of heating up hemp fibers to extremely high temperatures. Can be used as a fuel alternative for the wood-fired generator or as the base for other products.</description>
		<graphicData>
			<texPath>Things/Items/MSI_carbonizedHemp</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
    	<soundInteract>Wood_Drop</soundInteract>
    	<soundDrop>Wood_Drop</soundDrop>
    	<useHitPoints>false</useHitPoints>
		<statBases>
			<MarketValue>5</MarketValue>
			<Mass>0.1</Mass>
		</statBases>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>IndustrialSmokeleafProcessing</li>
		</researchPrerequisites>
	</ThingDef>

  <!--========== Hemp Nanosheet ==========-->

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>HempNanosheet</defName>
		<label>hemp nanosheet</label>
		<description>A Glitterworld material made from carbonized hemp fibers. It is used in advanced equipment for it's highly conductive properties.  It can also be used to build various items.</description>
		<graphicData>
			<texPath>Things/Items/MSI_nanosheet</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<color>(72,74,1)</color>
		</graphicData>
    	<soundInteract>Metal_Drop</soundInteract>
    	<soundDrop>Metal_Drop</soundDrop>
    	<useHitPoints>false</useHitPoints>
		<statBases>
			<MarketValue>125</MarketValue>
			<Mass>0.25</Mass>
      		<StuffPower_Armor_Sharp>1.35</StuffPower_Armor_Sharp>
      		<StuffPower_Armor_Blunt>0.75</StuffPower_Armor_Blunt>
      		<StuffPower_Armor_Heat>1.15</StuffPower_Armor_Heat>
      		<StuffPower_Insulation_Cold>2</StuffPower_Insulation_Cold>
      		<StuffPower_Insulation_Heat>1</StuffPower_Insulation_Heat>
      		<SharpDamageMultiplier>1.3</SharpDamageMultiplier>
      		<BluntDamageMultiplier>1.1</BluntDamageMultiplier>
		</statBases>
		<stuffProps>
			<categories>
				<li>Metallic</li>
			</categories>
			<color>(72,74,1)</color>
			<commonality>1.4</commonality>
      		<soundImpactStuff>BulletImpact_Metal</soundImpactStuff>
      		<soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
      		<soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
			<statFactors>
        		<MaxHitPoints>1.0</MaxHitPoints>
				<WorkToMake>1.5</WorkToMake>
       			<Flammability>0.0</Flammability>
        		<MeleeWeapon_CooldownMultiplier>0.7</MeleeWeapon_CooldownMultiplier>
			    <ArmorRating_Blunt>1.1</ArmorRating_Blunt>
			    <ArmorRating_Sharp>1.2</ArmorRating_Sharp>
			</statFactors>	
		</stuffProps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>SmokeleafGlittertech</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== Hempoline ==========-->
	
	<ThingDef ParentName="MSI_OrganicProductBase">
		<defName>Hempoline</defName>
		<label>hempoline</label>
		<description>A safe, stable fuel produced from refined smokeleaf stalks or seeds. Non-volatile, but goes bad eventually.</description>
		<graphicData>
			<texPath>Things/Items/MSI_hempoline</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<stackLimit>400</stackLimit>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<MarketValue>5</MarketValue>
			<Mass>0.05</Mass>
			<DeteriorationRate>1.0</DeteriorationRate>
		</statBases>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>120</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
		</comps>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
		<researchPrerequisites>
			<li>IndustrialSmokeleafProcessing</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== CBD ==========-->
	  
	<ThingDef ParentName="MSI_ResourceBase">
		<defName>CBD</defName>
		<label>CBD</label>
		<description>A compound identified in the smokeleaf plant that has vast medical potential. CBD can be used on it's own or combined with other medicine to create an even more effective form of treatment.</description>
		<thingClass>Medicine</thingClass>
		<graphicData>
			<texPath>Things/Items/MSI_CBD</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<statBases>
			<MaxHitPoints>30</MaxHitPoints>
			<MarketValue>30</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>1.0</Flammability>
			<MedicalPotency>0.75</MedicalPotency>
			<DeteriorationRate>6</DeteriorationRate>
			<WorkToMake>750</WorkToMake>
		</statBases>
		<thingCategories>
			<li>Medicine</li>
		</thingCategories>
		<costList>
			<SmokeleafBuds>25</SmokeleafBuds>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>MedicinalsLab</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>3</Crafting>
				<Medicine>5</Medicine>
			</skillRequirements>
		</recipeMaker>
		<researchPrerequisites>
			<li>SmokeleafPharmacology</li>
		</researchPrerequisites>
	</ThingDef>
  
  <!--========== Infused Medicine ==========-->  

	<ThingDef ParentName="MSI_ResourceBase">
		<defName>InfusedMedicine</defName>
		<label>infused medicine</label>
		<description>Medicine infused with CBD. More effective than regular medicine.</description>
		<thingClass>Medicine</thingClass>
		<graphicData>
			<texPath>Things/Items/MSI_medicine</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<statBases>
			<MaxHitPoints>60</MaxHitPoints>
			<MarketValue>50</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>1.0</Flammability>
			<MedicalPotency>1.60</MedicalPotency>
			<DeteriorationRate>6</DeteriorationRate>
			<WorkToMake>750</WorkToMake>
		</statBases>
		<costList>
			<MedicineIndustrial>1</MedicineIndustrial>
			<CBD>1</CBD>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>MedicinalsLab</li>
			</recipeUsers>
			<skillRequirements>
				<Crafting>6</Crafting>
				<Medicine>12</Medicine>
			</skillRequirements>
		</recipeMaker>
		<researchPrerequisites>
			<li>SmokeleafPharmacology</li>
		</researchPrerequisites>
    	<thingCategories>
      		<li>Medicine</li>
    	</thingCategories>
	</ThingDef>  
	  
</Defs>