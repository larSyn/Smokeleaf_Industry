<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!--========== Bake Bread ==========-->
	
	<Operation Class="PatchOperationSequence">
		<success>Always</success>
		<operations>
			<li Class="ModCheck.isModLoaded">
				<modName>VGP Garden Gourmet</modName>
				<yourMod>Smokeleaf Industry</yourMod>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/RecipeDef[defName = "BakeBread"]/ingredients/li[1]/filter/thingDefs</xpath>
				<value>
					<li>HempFlour</li>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/RecipeDef[defName = "BakeBread"]/fixedIngredientFilter/thingDefs</xpath>
				<value>
					<li>HempFlour</li>
				</value>
			</li>
		</operations>
	</Operation>
	
